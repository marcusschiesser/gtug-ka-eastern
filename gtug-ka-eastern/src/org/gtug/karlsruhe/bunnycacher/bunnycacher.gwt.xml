<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='bunnycacher'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.gtug.karlsruhe.bunnycacher.client.Application'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='common'/>

  <!-- ClientBundle support -->
  <inherits name="com.google.gwt.resources.Resources" />
  <!-- Inherit the Google Maps stuff                     -->
  <inherits name='com.googlecode.maps3.Maps3' />
  <!-- Inherit the Phonegap connector stuff                     -->
  <inherits name='org.gtug.karlsruhe.phonegap.Phonegap' />
  <!-- Include Google Maps API  -->
  <script src="http://maps.google.com/maps/api/js?sensor=false" />
  <!-- Include Application Styles (only needed until GWT supports -webkit prefixes)  -->
  <stylesheet src="styles.css" />
</module>
